{
  "name": "cat-api",
  "version": "1.0.0",
  "description": "API to process HTTP requests to access information on local cats",
  "main": "index.js",
  "scripts": {
    "start": "node src/buildServer.js",
    "test": "mocha 'test/catRoutes.test.js' --exit",  
    "jest": "jest --forceExit",
    "catRoutes": "jest 'test/catRoutes.test.js'  --detectOpenHandles --forceExit",
    "breedRoutes": "jest 'test/breedRoutes.test.js'  --detectOpenHandles --forceExit",
    "validation": "jest 'test/validationFunctions.test.js' --detectOpenHandles --forceExit",
    "detectOH": "jest 'test/catRoutes.test.js' --watch --runInBand --detectOpenHandles --forceExit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skwong25/cat-api.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/skwong25/cat-api/issues"
  },
  "homepage": "https://github.com/skwong25/cat-api#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "jest": "^26.4.2",
    "mocha": "^8.1.1",
    "morgan": "^1.10.0",
    "shortid": "^2.2.15",
    "should": "^13.2.3"
  },
  "devDependencies": {
    "eslint": "^7.6.0",
    "supertest": "^4.0.2"
  }
}
